{
    "version": 2,
    "builds": [
        {
            "src": "backend/api.py",
            "use": "@vercel/python"
        },
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/api.py"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ],
    "env": {
        "GOOGLE_API_KEY": "@google_api_key",
        "TAVILY_API_KEY": "@tavily_api_key"
    }
}